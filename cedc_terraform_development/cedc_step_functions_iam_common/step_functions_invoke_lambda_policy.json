{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowStepFunctionsToInvokeLambda",
        "Effect": "Allow",
        "Action": [
          "lambda:InvokeFunction",
          "glue:StartJobRun",
          "glue:GetJobRun",
          "glue:GetJobRuns",
          "glue:GetJob"
        ],
        "Resource": [
          "*"
        ]
      }
    ]
  }