{
  "Comment": "A Hello World example of the Amazon States Language using a Glue Job",
  "StartAt": "GlueJob",
  "States": {
    "GlueJob": {
      "Type": "Task",
      "Resource": "arn:aws:states:::glue:startJobRun.sync",
      "Parameters": {
        "JobName": "cicd-gitaction-glue-job"
      },
      "End": true
    }
  }
}
